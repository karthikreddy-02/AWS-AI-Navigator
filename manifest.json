{
  "manifest_version": 3,
  "name": "AWS Navigator",
  "version": "1.0",
  "description": "Navigate AWS Console with ease using OpenAI.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
